{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production bun --env-file=./.env.production.local build ./src/index.ts --bytecode --minify --outdir ./dist --target bun",
    "compile": "NODE_ENV=production bun --env-file=./.env.production.local build ./src/index.ts --bytecode --compile --minify --outfile ./dist/index --target bun",
    "dev": "NODE_ENV=development bun --env-file=./.env.development.local --watch run ./src/index.ts",
    "type-check": "tsc"
  },
  "dependencies": {},
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@types/bun": "^1.1.10",
    "typescript": "^5.6.2"
  },
  "prettier": "@kikiutils/prettierrc"
}
