    "cli-progress": "^3.12.0"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^2.2.0",
    "@types/bun": "^1.1.6",
    "@types/cli-progress": "^3.11.6",
    "typescript": "^5.5.3"
  },
  "prettier": "@kikiutils/prettierrc"
}
